{
  "preset": "ts-jest",
  "testEnvironment": "node",
  "transform": {
    "^.+\\.(ts|tsx)$": "ts-jest"
  },
  "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(tsx?|jsx?)$",
  "moduleFileExtensions": ["ts", "js", "json", "node"],
  "moduleNameMapper": {
    "^@routes$": "<rootDir>/src/routes/index.ts",
    "^@routes/(.*)$": "<rootDir>/src/routes/$1"
  }
}
